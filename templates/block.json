{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "dd-blocks/${blockName}",
	"version": "0.1.0",
	"title": "${blockName}",
	"category": "widgets",
	"icon": "smiley",
	"description": "This is an example block, feel free to copy this scaffolding for each new block",
	"supports": {
		"html": false
	},
	"textdomain": "multi-blocks",
	"editorScript": "file:../../build/${blockName}.js",
	"editorStyle": "file:../../build/${blockName}.css",
	"style": "file:../../build/style-${blockName}.css"
}
